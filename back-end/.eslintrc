{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "camelcase": 1,
    "no-console": 1,
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-empty": 1,
    "no-plusplus": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}